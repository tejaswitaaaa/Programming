#include<bits/stdc++.h>
#include <stdio.h>
using namespace std;
int main()
{
   vector<int> v;
    int n;
    cin>>n;
    for(int i=0; i<n; i++){
        int x;
        cin>>x;
        v.push_back(x);
        for(int i; i< v.size(); i++){
            cout<<v[i]<<" "<<"h";
        }
    }
    
    return 0;
}
